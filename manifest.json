{
    "manifest_version": 3,
    "name": "Ad Skipper",
    "version": "1.0",
    "description": "Automatically clicks the skip ad button on supported websites.",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "48": "icon48.jpeg",
      "128": "icon128.jpeg"
    },
    "action": {
      "default_icon": {
        "48": "icon48.jpeg",
        "128": "icon128.jpeg"
      },
      "default_title": "Ad Skipper"
    }
  }
  